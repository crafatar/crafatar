{
  "name": "crafatar",
  "version": "1.0.0",
  "private": true,
  "description": "A blazing fast API for Minecraft faces!",
  "contributors": [
    { "name": "jomo", "url": "https://github.com/jomo"},
    { "name": "Jake", "url": "https://github.com/Jake0oo0"}
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crafatar/crafatar"
  },
  "issues": {
    "url": "https://github.com/crafatar/crafatar/issues"
  },
  "keywords": [
    "minecraft",
    "avatar"
  ],
  "scripts": {
    "postinstall": "cp 'config.example.js' 'config.js'",
    "start": "node www.js",
    "test": "mocha",
    "test-travis": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "engines": {
    "iojs": "2.0.x"
  },
  "dependencies": {
    "canvas": "^1.2.9",
    "crc": "~3.3.0",
    "jade": "~1.11.0",
    "lwip": "~0.0.7",
    "mime": "~1.3.4",
    "node-df": "crafatar/node-df",
    "redis": "~2.0.0",
    "request": "~2.64.0",
    "toobusy-js": "~0.4.2"
  },
  "devDependencies": {
    "coveralls": "~2.11.2",
    "istanbul": "~0.3.20",
    "mocha": "~2.3.3",
    "mocha-lcov-reporter": "~1.0.0"
  }
}
